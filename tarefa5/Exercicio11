package tarefa05;

import java.util.Scanner;

public class Exercicio11 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Digite o nÃºmero da sua conta ");
		int contacliente = sc.nextInt();

		System.out.println("Diite seu saldo ");
		int saldo = sc.nextInt();

		System.out.println("Digite seu saldo no debito");
		int debito = sc.nextInt();

		System.out.println("Digite o seu saldo em credito desponivel");
		int credito = sc.nextInt();

		int saldoatual = saldo - debito + credito;

		if (saldoatual >= 0) {
			System.out.println("Saldo positivo :" + saldoatual);
		} else {
			System.out.println("Saldo negativo :" );
		}
		sc.close();

	}

}
